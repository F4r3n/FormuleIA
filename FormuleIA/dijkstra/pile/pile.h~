#ifndef PILE_H
#define PILE_H

#include <stdlib.h>
#include <stdio.h>

typedef struct _Noeud
{
	struct _Noeud *suivant;
	int pos;
}Noeud;

typedef struct _Pile
{
	Noeud *head;
	Noeud *courant;
	int taille;
}Pile;

int videPile(Pile *positionsPile);
void empiler(Pile *positionsPile, int posi);
int depiler(Pile *positionsPile);
Pile *creerPile();

#endif
